    <quoteString>N</quoteString>
    <sql>DECLARE @minContrato INT
DECLARE @maxContrato INT
DECLARE @contratoKey VARCHAR(50)
DECLARE @contratoCodigo VARCHAR(10)
DECLARE @banco VARCHAR(50)
DECLARE @linhagemBanco VARCHAR(2)

DECLARE @sql NVARCHAR(4000)
DECLARE @sqlParams NVARCHAR(2000)
--BEGIN TRAN

DECLARE @top INT = 50000

SET @sqlParams = N'
	@contratoKey VARCHAR(50),
	@contratoCodigo VARCHAR(10)
'

DECLARE @contrato AS TABLE (
	contratoLinha INT IDENTITY(1,1),
	contratoKey INT,
	contratoCodigo VARCHAR(10),
	contratoBanco VARCHAR(50),
	linhagemBanco VARCHAR(2)			
)

INSERT INTO @contrato
		SELECT
			contratoKey
			,contratoCodigo
			,contratoBanco
			,CAST(linhagemBanco AS VARCHAR(2)) linhagemBanco
		FROM dbo.Contrato  
		WHERE ativo = 1 AND sits = 0

SELECT
	@minContrato = MIN(contratoLinha)
	,@maxContrato = MAX(contratoLinha)
FROM @contrato

WHILE @minContrato &lt;= @maxContrato	
BEGIN
	SELECT
		@banco = QUOTENAME(contratoBanco)
		,@contratoKey = contratoKey
		,@contratoCodigo = contratoCodigo
		,@linhagemBanco = linhagemBanco
	FROM @contrato
	WHERE contratoLinha = @minContrato

	SET @sql = N'
		UPDATE TOP (' + CAST(@top as VARCHAR(10)) + ') ' +  @banco + '..[tbl_TrfCtrIns]
		SET trf_bit_processado = 1
		WHERE trf_bit_processado = 0
	'

	EXECUTE sp_executesql @statement = @sql

	SET @sql = N'
		SET TRANSACTION ISOLATION LEVEL READ UNCOMMITTED
		INSERT INTO SCO_DW.dbo.FatoTrafegoImagem
		SELECT 
			'+IIF(@linhagemBanco = '16', 'T.trf_big_ID', 'T.trf_itg_ID')+' AS [trf_big_ID]
			,e.eqp_itg_Id		AS [eqp_itg_Id]
			,meq.meq_tyi_ID		AS [meq_tyi_ID]
			,'+IIF(@linhagemBanco = '16', 'i.trf_big_ID', 'i.trf_itg_ID')+' AS [img_ID]
		FROM ' + @banco + '..tbl_trf T WITH(NOLOCK) 
		LEFT JOIN ' + @banco + '..tbl_eqp e WITH(NOLOCK) 
			ON T.eqp_itg_Id = e.eqp_itg_ID
		LEFT JOIN ' + @banco + '..[tbl_mdlEqp] meq WITH(NOLOCK)
			ON e.meq_tyi_ID = meq.meq_tyi_ID
		LEFT JOIN ' + @banco + '..tbl_Img i WITH(NOLOCK) '
		+
			IIF(@linhagemBanco = '16', ' ON i.trf_big_ID = t.trf_big_ID ', ' ON i.trf_itg_ID = t.trf_big_ID ')
		+
		'
		WHERE ' + IIF(@linhagemBanco = '16', 'T.trf_big_ID', 'T.trf_itg_ID') + ' IN (SELECT trf_big_Id FROM ' + @banco + '..tbl_TrfCtrIns WITH(NOLOCK) WHERE trf_bit_processado = 1)	
	'

	EXECUTE sp_executesql	@statement = @sql
							,@params = @sqlParams
							,@contratoKey = @contratoKey
							,@contratoCodigo = @contratoCodigo

	BEGIN
		SET @sql = N'
			DELETE FROM ' + @banco + '..[tbl_TrfCtrIns]	
			WHERE trf_bit_processado = 1
		'

		EXECUTE sp_executesql @statement = @sql
	END

	SET @minContrato = @minContrato + 1

END</sql>
